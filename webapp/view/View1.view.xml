<mvc:View controllerName="mentoria.ka.fiori.zpraticamodel1bfs.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns:form="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
    <headerContent>
        <Toolbar >
        <ToolbarSpacer />
            <Button text="+ Novo" type="Success" icon="sap-icon://add-employee" press="onNovoPress" />
        </Toolbar>
    </headerContent>
        <content>
            <Panel headerText="Cadastros" class="sapUiResponsiveMargin" width="auto">
                <List items="{path: 'model2>/Cadastros'}" backgroundDesign="Solid">
                    <items>
                        <ObjectListItem title="{Cadastros>Nome}" >
                        
                        </ObjectListItem>
                    </items>
                </List>
            </Panel>
        </content>
    </Page>
</mvc:View>
